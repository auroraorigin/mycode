.searchInput {
    height: 90rpx;
    width: 100%;
    padding: 10rpx 15rpx;
    position: relative;
    z-index: 1;
}

.searchInput navigator {
    height: 100%;
    display: flex;
    flex-direction: row;
    align-items: center;
    border-style: solid;
    border-color: #ededed;
    border-radius: 16rpx;
    border-width: 5rpx;
    box-shadow:  0px 0px 7px #ededed;
}

.searchInput image {
    width: 8%;
}

.searchInput text{
    color: #666;
}